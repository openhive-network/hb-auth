{
  "name": "@hiveio/hb-auth",
  "version": "1.0.0",
  "description": "Browser and Node.js auth library that uses Beekeeper",
  "type": "module",
  "main": "dist/hb-auth.js",
  "module": "dist/hb-auth.mjs",
  "typings": "dist/hb-auth.d.ts",
  "scripts": {
    "build": "npm run clean && rollup -c --bundleConfigAsCjs",
    "clean": "rm -rf ./dist",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "prebuild": "npm run lint && npm run test",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.syncad.com/hive/beekeeper-js"
  },
  "keywords": [
    "beekeper",
    "beekeper-js",
    "blockchain",
    "hive"
  ],
  "author": "",
  "license": "",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "babel-jest": "^29.6.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "prettier": "3.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@hive/beekeeper": "^0.0.1-alpha.0"
  }
}
